{
  "name": "fakeZhiHu",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.4",
    "@types/react-test-renderer": "^15.4.2",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "^1.9.1",
    "enzyme": "^2.8.2",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "tslint": "^5.0.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "build": "rm -rf build && yarn tsc -- --watch"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "preset": "react-native",
    "collectCoverage": true,
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/config/jest-setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/config/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/*.test.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/react": "15.0.27",
    "@types/react-dom": "^15.5.0",
    "@types/react-native": "^0.44.10",
    "@types/react-native-swiper": "^1.5.5",
    "@types/react-navigation": "^1.0.0",
    "@types/react-redux": "^4.4.37",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.6",
    "date-fns": "^1.28.5",
    "react": "16.0.0-alpha.6",
    "react-dom": "16.0.0-alpha.6",
    "react-native": "0.43.0",
    "react-native-swiper": "^1.5.4",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.3",
    "react-redux-typescript": "^2.3.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-observable": "^0.14.1",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.3.1"
  }
}
